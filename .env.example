 # ─── GCP ───────────────────────────────────────────
GCP_PROJECT_ID=your-project-id
GCP_REGION=europe-west1

# ─── Cloud SQL ─────────────────────────────────────
# Local : via Cloud SQL Auth Proxy (port 5432)
DATABASE_URL=postgresql+asyncpg://postgres:password@localhost:5432/dd_intelligence
# Production (Cloud Run via Unix socket) :
# DATABASE_URL=postgresql+asyncpg://postgres:password@/dd_intelligence?host=/cloudsql/YOUR_PROJECT:europe-west1:dd-postgres

# ─── Cache (pas de Redis — cachetools + PostgreSQL) ─
CACHE_DEFAULT_TTL=300
CACHE_MAX_SIZE=500

# ─── Cloud Storage ─────────────────────────────────
GCS_RAW_BUCKET=dd-raw-data-your-project
GCS_PROCESSED_BUCKET=dd-processed-your-project

# ─── Gemini API ────────────────────────────────────
GEMINI_API_KEY=your-gemini-api-key
GEMINI_MODEL_CHAT=gemini-1.5-flash
GEMINI_MODEL_EMBED=text-embedding-004

# ─── APIs Données Françaises ───────────────────────
DINUM_API_URL=https://recherche-entreprises.api.gouv.fr
INSEE_API_KEY=
INFOGREFFE_API_KEY=
BODACC_API_URL=https://bodacc-datadila.opendatasoft.com/api/explore/v2.1
NEWS_API_KEY=

# ─── App ───────────────────────────────────────────
ENVIRONMENT=development
SECRET_KEY=change-me-in-production
DEBUG=true
ALLOWED_ORIGINS=http://localhost:3000